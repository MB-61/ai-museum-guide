# Gemini API Configuration
# Copy this file to .env and add your API keys

# Main API key (always used first)
GOOGLE_API_KEY=your_google_api_key_here

# Additional API keys for rotation
GOOGLE_API_KEY_1=your_google_api_key_1_here
GOOGLE_API_KEY_2=your_google_api_key_2_here

# Timeout for api key change
LLM_TIMEOUT=30

# LLM Model
LLM_MODEL=gemini-2.5-flash

# Azure Speech API (for TTS with lip-sync)
AZURE_SPEECH_KEY=your_azure_speech_key_here
AZURE_SPEECH_REGION=swedencentral
